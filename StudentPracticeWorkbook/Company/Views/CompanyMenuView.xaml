<UserControl x:Class="Company.Views.CompanyMenuView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Border Margin="5" DockPanel.Dock="Right" BorderThickness="0.3" BorderBrush="DarkGray">
        <StackPanel>
            <Button Margin="5" Content="Wydziały" Command="{Binding NavigationCommand}" CommandParameter="Company.Views.CompanyDepartmentsView"></Button>
            <Button Margin="5" Content="Praktykanci" Command="{Binding NavigationCommand}" CommandParameter="Company.Views.CompanyInternsView"></Button>
            <Button Margin="5" Content="Firma" Command="{Binding NavigationCommand}" CommandParameter="Company.Views.CompanyAboutView"></Button>
        </StackPanel>
    </Border>
</UserControl>

